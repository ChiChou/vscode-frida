{
	"name": "vscode-frida",
	"icon": "icon.png",
	"publisher": "codecolorist",
	"repository": {
		"type": "git",
		"url": "https://github.com/ChiChou/vscode-frida"
	},
	"displayName": "frida Workbench",
	"description": "Unofficial frida workbench for VSCode",
	"version": "0.6.2",
	"engines": {
		"vscode": "^1.69.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Frida",
			"properties": {
				"frida.githubToken": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"description": "Github token to download frida-server, etc."
				},
				"frida.decryptOutput": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"description": "Preferred directory to save ipa"
				}
			}
		},
		"commands": [
			{
				"command": "frida.ps.refresh",
				"title": "Refresh Processes",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.apps.refresh",
				"title": "Refresh Apps",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.remote.add",
				"title": "Connect Remote Device...",
				"icon": {
					"light": "resources/light/add.svg",
					"dark": "resources/dark/add.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.remote.remove",
				"title": "Disconnect Remote Device...",
				"icon": {
					"light": "resources/light/del.svg",
					"dark": "resources/dark/del.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.attach",
				"title": "Attach to",
				"icon": {
					"light": "resources/light/attach.svg",
					"dark": "resources/dark/attach.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.device.copy",
				"title": "Copy Device ID"
			},
			{
				"command": "frida.bundle.copy",
				"title": "Copy Bundle ID"
			},
			{
				"command": "frida.name.copy",
				"title": "Copy Process Name"
			},
			{
				"command": "frida.spawn",
				"title": "Spawn",
				"icon": {
					"light": "resources/light/statusRun.svg",
					"dark": "resources/dark/statusRun.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.spawn.suspended",
				"title": "Spawn Suspended",
				"category": "Frida"
			},
			{
				"command": "frida.kill",
				"title": "End Process",
				"category": "Frida"
			},
			{
				"command": "frida.debug.setup",
				"title": "Create Debugger Configuration",
				"category": "Frida"
			},
			{
				"command": "frida.syslog",
				"title": "View Debug Logs",
				"icon": {
					"light": "resources/light/statusRun.svg",
					"dark": "resources/dark/statusRun.svg"
				},
				"category": "Frida"
			},
			{
				"command": "frida.syslog.vacuum",
				"title": "Close All Console",
				"category": "Frida"
			},
			{
				"command": "frida.external.objection",
				"title": "Objection",
				"category": "Frida"
			},
			{
				"command": "frida.external.dump",
				"title": "Dump app",
				"category": "Frida"
			},
			{
				"command": "frida.external.lldb",
				"title": "Debug with lldb",
				"category": "iOS"
			},
			{
				"command": "frida.external.setuplldb",
				"title": "Setup LLDB debugserver",
				"category": "iOS"
			},
			{
				"command": "frida.device.androidserver",
				"title": "Start frida-server",
				"category": "Android"
			},
			{
				"command": "frida.external.shell",
				"title": "Open Shell",
				"category": "SSH over USB"
			},
			{
				"command": "frida.external.copyid",
				"title": "Install SSH Public Key",
				"category": "SSH over USB"
			},
			{
				"command": "frida.external.sshkeygen",
				"title": "Generate SSH Keypair",
				"category": "SSH over USB"
			},
			{
				"command": "frida.boilerplate.agent",
				"title": "Create Frida Agent",
				"category": "Frida Boilerplate"
			},
			{
				"command": "frida.boilerplate.module",
				"title": "Create Frida CModule",
				"category": "Frida Boilerplate"
			},
			{
				"command": "frida.typing.init",
				"title": "Get typing hint for frida",
				"category": "Frida"
			},
			{
				"command": "frida.snippet.execute",
				"title": "Execute",
				"category": "Frida",
				"icon": "resources/run.svg"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "frida-sidebar",
					"title": "Frida Utilities",
					"icon": "resources/icon.svg"
				}
			]
		},
		"views": {
			"frida-sidebar": [
				{
					"id": "fridaApps",
					"name": "Apps"
				},
				{
					"id": "fridaPs",
					"name": "Processes"
				}
			]
		},
		"menus": {
			"commandPalette": [
				{
					"command": "frida.boilerplate.agent"
				},
				{
					"command": "frida.boilerplate.module"
				},
				{
					"command": "frida.ps.refresh"
				},
				{
					"command": "frida.apps.refresh"
				},
				{
					"command": "frida.remote.add"
				},
				{
					"command": "frida.remote.remove"
				},
				{
					"command": "frida.typing.init"
				},
				{
					"command": "frida.external.objection",
					"when": "false"
				},
				{
					"command": "frida.external.dump",
					"when": "false"
				},
				{
					"command": "frida.external.lldb",
					"when": "false"
				},
				{
					"command": "frida.external.setuplldb",
					"when": "false"
				},
				{
					"command": "frida.external.shell",
					"when": "false"
				},
				{
					"command": "frida.external.copyid",
					"when": "false"
				},
				{
					"command": "frida.external.sshkeygen"
				},
				{
					"command": "frida.syslog",
					"when": "false"
				},
				{
					"command": "frida.attach",
					"when": "false"
				},
				{
					"command": "frida.kill",
					"when": "false"
				},
				{
					"command": "frida.spawn",
					"when": "false"
				},
				{
					"command": "frida.debug.setup",
					"when": "false"
				},
				{
					"command": "frida.spawn.suspended",
					"when": "false"
				},
				{
					"command": "frida.bundle.copy",
					"when": "false"
				},
				{
					"command": "frida.name.copy",
					"when": "false"
				},
				{
					"command": "frida.device.copy",
					"when": "false"
				},
				{
					"command": "frida.snippet.execute",
					"when": "editorLangId == javascript && terminalFocus"
				}
			],
			"editor/title": [
				{
					"when": "resourceLangId == javascript",
					"command": "frida.snippet.execute",
					"group": "navigation@1"
				}
			],
			"view/title": [
				{
					"command": "frida.remote.add",
					"when": "view == fridaApps",
					"group": "navigation@1"
				},
				{
					"command": "frida.apps.refresh",
					"when": "view == fridaApps",
					"group": "navigation@2"
				},
				{
					"command": "frida.remote.add",
					"when": "view == fridaPs",
					"group": "navigation@1"
				},
				{
					"command": "frida.ps.refresh",
					"when": "view == fridaPs",
					"group": "navigation@2"
				}
			],
			"view/item/context": [
				{
					"command": "frida.remote.remove",
					"when": "view == fridaApps && viewItem =~ /^device\\|socket@/",
					"group": "inline"
				},
				{
					"command": "frida.remote.remove",
					"when": "view == fridaPs && viewItem =~ /^device\\|socket@/",
					"group": "inline"
				},
				{
					"command": "frida.attach",
					"when": "view == fridaApps && viewItem == 'app|running'",
					"group": "inline"
				},
				{
					"command": "frida.attach",
					"when": "view == fridaPs && viewItem == 'process|running'",
					"group": "inline"
				},
				{
					"command": "frida.spawn",
					"when": "view == fridaApps && viewItem =~ /^app\\|/",
					"group": "inline"
				},
				{
					"command": "frida.debug.setup",
					"when": "view == fridaApps && viewItem =~ /^app\\|/",
					"group": "1_frida_info@1"
				},
				{
					"command": "frida.debug.setup",
					"when": "view == fridaPs && viewItem =~ /^process\\|/",
					"group": "1_frida_info@1"
				},
				{
					"command": "frida.bundle.copy",
					"when": "view == fridaApps && viewItem =~ /^app\\|/",
					"group": "1_frida_info@1"
				},
				{
					"command": "frida.name.copy",
					"when": "view == fridaPs && viewItem =~ /^process\\|/",
					"group": "1_frida_info@1"
				},
				{
					"command": "frida.kill",
					"when": "viewItem =~ /\\|running$/",
					"group": "2_frida_process@2"
				},
				{
					"command": "frida.spawn.suspended",
					"when": "view == fridaApps && viewItem =~ /^app\\|/",
					"group": "2_frida_process@1"
				},
				{
					"command": "frida.external.lldb",
					"when": "!isWindows && view == fridaApps && viewItem =~ /^app\\|/ && viewItem =~ /\\|ios$/",
					"group": "2_frida_process@2"
				},
				{
					"command": "frida.external.lldb",
					"when": "!isWindows && view == fridaPs && viewItem =~ /^process\\|/ && viewItem =~ /\\|ios$/",
					"group": "2_frida_process@2"
				},
				{
					"command": "frida.syslog",
					"when": "view == fridaApps && viewItem =~ /^app\\|/",
					"group": "3_frida_logger@1"
				},
				{
					"command": "frida.syslog",
					"when": "view == fridaPs && viewItem =~ /^process\\|/",
					"group": "3_frida_logger@1"
				},
				{
					"command": "frida.external.objection",
					"when": "view == fridaApps && viewItem =~ /^app\\|/",
					"group": "4_frida_external@2"
				},
				{
					"command": "frida.external.dump",
					"when": "view == fridaApps && viewItem =~ /^app\\|/ && viewItem =~ /\\|(android|ios)$/",
					"group": "4_frida_external@3"
				},
				{
					"command": "frida.external.shell",
					"when": "viewItem =~ /^device\\|\\w+\\|(android|ios)$/ || viewItem =~ /^device\\|\\local/",
					"group": "1_frida_dev_external@1"
				},
				{
					"command": "frida.device.copy",
					"when": "viewItem =~ /^device\\|/",
					"group": "1_frida_dev_setup@1"
				},
				{
					"command": "frida.device.androidserver",
					"when": "viewItem =~ /^device\\|\\w+\\|android$/",
					"group": "2_frida_dev_setup@2"
				},
				{
					"command": "frida.external.setuplldb",
					"when": "viewItem =~ /^device\\|\\w+\\|ios$/",
					"group": "2_frida_dev_setup@2"
				},
				{
					"command": "frida.external.copyid",
					"when": "viewItem =~ /^device\\|\\w+\\|ios$/",
					"group": "2_frida_dev_setup@3"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.6.4",
		"@types/vscode": "^1.69.1",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"tslint": "^6.1.3",
		"typescript": "^4.7.4",
		"vscode-test": "^1.6.1"
	},
	"dependencies": {
		"@octokit/rest": "^19.0.7",
		"lookpath": "^1.2.2"
	},
	"extensionDependencies": [
		"ms-python.python"
	]
}
